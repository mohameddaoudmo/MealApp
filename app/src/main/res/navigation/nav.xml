<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/home2">
    <fragment
        android:id="@+id/home2"
        android:name="com.example.mealapp.home.Home.View.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/categories"
        android:name="com.example.mealapp.home.Home.View.Categories"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" />
    <fragment
        android:id="@+id/countries"
        android:name="com.example.mealapp.home.Home.View.Countries"
        android:label="fragment_countries"
        tools:layout="@layout/fragment_countries" >
        <action
            android:id="@+id/action_countries_to_country"
            app:destination="@id/country" />
    </fragment>
    <fragment
        android:id="@+id/country"
        android:name="com.example.mealapp.Country"
        android:label="fragment_country"
        tools:layout="@layout/fragment_country" />
</navigation>